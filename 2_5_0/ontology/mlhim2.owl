    </description>
        <creator rdf:datatype="&xs;string">Timothy W. Cook</creator>
        <rdfs:isDefinedBy rdf:datatype="&xs;anyURI">http://www.mlhim.org/</rdfs:isDefinedBy>
    </owl:Ontology>
    


