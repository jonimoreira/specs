    <!-- http://www.mlhim.org/ns/mlhim2/CCDInstance -->

    <owl:Class rdf:about="&mlhim2;CCDInstance">
        <rdfs:label xml:lang="en-us">Concept Constraint Definition Instance</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;MLHIM2"/>
        <description xml:lang="en-us">
      A set of selected PluggableCMs that are constraints on the RM components (CoreCMs) in order to represent a domain concept. 
      In the implementation language there may be additional syntactic conventions required. 
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/ConceptModel -->

    <owl:Class rdf:about="&mlhim2;ConceptModel">
        <rdfs:label xml:lang="en-us">Concept Model</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;MLHIM2"/>
        <description xml:lang="en-us">
      A model of an abstract concept.
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/CoreCM -->

    <owl:Class rdf:about="&mlhim2;CoreCM">
        <rdfs:label xml:lang="en-us">Core Concept Model</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;ConceptModel"/>
        <description xml:lang="en-us">
      A composable model contained in a reference model. A CoreCM represents a specific
      core type of component that further contains elements with base datatypes and/or other CoreCMs to define its structure.
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/CoreCS -->

    <owl:Class rdf:about="&mlhim2;CoreCS">
        <rdfs:label xml:lang="en-us">Core Concept Symbol</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;Symbol"/>
        <description xml:lang="en-us">
      A CoreCS represents a CoreCM in instance data. In practice, it is usually substituted for by a PluggableCS.
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/DataInstance -->

    <owl:Class rdf:about="&mlhim2;DataInstance">
        <rdfs:label xml:lang="en-us">Data Instance</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;MLHIM2"/>
        <description xml:lang="en-us">
      A set of data items (e.g. a document) that conforms restrictions described in a CCD Instance.
      It has not been tested for validation.
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/DataInstanceError -->

    <owl:Class rdf:about="&mlhim2;DataInstanceError">
        <rdfs:label xml:lang="en-us">Erroneous Data Instance</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;DataInstance"/>
        <description xml:lang="en-us">
      A set of data items that **DOES NOT** conform to the CCD Instance it represents **OR**
      it contains invalid data and does not contain one or more children of an Exception.
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/DataInstanceInvalid -->

    <owl:Class rdf:about="&mlhim2;DataInstanceInvalid">
        <rdfs:label xml:lang="en-us">Invalid Data Instance</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;DataInstance"/>
        <description xml:lang="en-us">
      A set of data items that conforms to a CCD Instance to represent an instance of that concept **AND** the data 
      values are **NOT** valid according to the CCD Instance constraints. 
      An InvalidDataInstance must contain one or more children of an Exception.      
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/DataInstanceValid -->

    <owl:Class rdf:about="&mlhim2;DataInstanceValid">
        <rdfs:label xml:lang="en-us">Valid Data Instance</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;DataInstance"/>
        <description xml:lang="en-us">
      A set of data items that conforms to a CCD Instance to represent an instance of that concept 
      **AND** the data values are valid according to the CCD Instance constraints.
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/Exception -->

    <owl:Class rdf:about="&mlhim2;Exception">
        <rdfs:label xml:lang="en-us">Exception</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;CoreCM"/>
        <description xml:lang="en-us">
      Indicates that some data is outside of the parameters defined by the CCD Instance.
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/MLHIM2 -->

    <owl:Class rdf:about="&mlhim2;MLHIM2">
        <rdfs:label xml:lang="en-us">MLHIM 2.x</rdfs:label>
        <description xml:lang="en-us">
      The top level class of MLHIM 2. 
      MLHIM 1.x was never successfully modeled in OWL and MLHIM 3 will likely be a pure OWL implementation; sometime in the distant future.
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/PluggableCM -->

    <owl:Class rdf:about="&mlhim2;PluggableCM">
        <rdfs:label xml:lang="en-us">Pluggable Concept Model</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;ConceptModel"/>
        <description xml:lang="en-us">
      The name given to a CoreCM that has been constrained for use in a CCD Instance. 
      Through the constraints, a PluggableCM defines a single domain concept based on syntactic data constraints as well 
      as specified semantics. 
      It is *pluggable* because it can be reused in multiple CCD Instances. 
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/PluggableCS -->

    <owl:Class rdf:about="&mlhim2;PluggableCS">
        <rdfs:label xml:lang="en-us">Pluggable Concept Symbol</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;Symbol"/>
        <description xml:lang="en-us">
      Represents a PluggableCM in instance data. Can be considered a data container for the components of a PluggableCM. 
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/RM -->

    <owl:Class rdf:about="&mlhim2;RM">
        <rdfs:label xml:lang="en-us">Reference Model</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;MLHIM2"/>
        <description xml:lang="en-us">
      A set of components (CoreCMs) to provide structural integrity for a domain concept. 
      Some CoreCMs are mandatory in CCDs and some are optional. Optionality is defined in each RM implementation.
    </description>
    </owl:Class>
    


    <!-- http://www.mlhim.org/ns/mlhim2/Symbol -->

    <owl:Class rdf:about="&mlhim2;Symbol">
        <rdfs:label xml:lang="en-us">Symbol</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;MLHIM2"/>
        <description xml:lang="en-us">
      Used to delineate or markup instance data. 
      In practice, it is usually a variable name or XML element name.
    </description>
    </owl:Class>

    <!-- http://www.mlhim.org/ns/mlhim2/DataPoint-->
    
    <owl:Class rdf:about="&mlhim2;DataPoint">
        <rdfs:label xml:lang="en-us">Data Point</rdfs:label>
        <rdfs:subClassOf rdf:resource="&mlhim2;Symbol"/>
        <description xml:lang="en-us">
            Represents a complete path in instance data defined by a Pluggable Concept Model.
            Beginning from the left at the data node and ending at the CCDInstance with the 'ccd-{UUID}' on the right.
            This does not include file, graph, server, etc. names.
        </description>
    </owl:Class>
    


</rdf:RDF>



