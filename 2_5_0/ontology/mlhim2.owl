        <rdfs:seeAlso rdf:resource="&dc;description"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.mlhim.org/ns/mlhim2/uniqueID -->

    <owl:AnnotationProperty rdf:about="&mlhim2;uniqueID">
