        '&gt;' and is_strictly_comparable_to ('==').
      </xs:documentation>
      <xs:appinfo>
        <owl:Class rdf:about="&mlhim2;DvOrderedType">
          <rdfs:isDefinedBy rdf:resource="http://mlhim.org/rm250_html/mlhim250_xsd_Complex_Type_mlhim2_DvOrderedType.html#DvOrderedType"/>
          <rdfs:subClassOf rdf:resource="&mlhim2;DvAnyType"/>
<!--          
          <!-\- Need to add a subclass for 'range' here.  How to express zero to unbounded? -\->
       
          <rdfs:subClassOf>
            <owl:Restriction>
              <owl:onProperty rdf:resource="&mlhim2;normal-status" />
              <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
          </rdfs:subClassOf>                    
-->          <rdfs:label>DvOrderedType</rdfs:label>
          <mlhim2:description xml:lang="en-us">
            Abstract type defining the concept of ordered values, which includes
            ordinals as well as true quantities. The implementations require the functions ‘&lt;’,
            '&gt;' and is_strictly_comparable_to ('==').
          </mlhim2:description>
          <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="&mlhim2;DvOrdinalType"/>
            <rdf:Description rdf:about="&mlhim2;DvQuantifiedType"/>
            <rdf:Description rdf:about="&mlhim2;DvTemporalType"/>
          </owl:disjointUnionOf>
          
        </owl:Class>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
