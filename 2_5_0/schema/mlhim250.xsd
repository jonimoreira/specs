          <xs:element maxOccurs="1" minOccurs="1" name="lower-bounded" type="xs:boolean">
            <xs:annotation>
              <xs:documentation xml:lang="en-us">
                Is the lower value of the interval bounded?
              </xs:documentation>
<!--              <xs:appinfo>
                <owl:DatatypeProperty rdf:about="&mlhim2;DvIntervalType/lower-bounded">
                  <rdfs:domain rdf:resource="&mlhim2;DvIntervalType"/>
                  <rdfs:range rdf:resource="&xsd;boolean"/>
                  <mlhim2:description xml:lang="en-us">
                    Is the lower value of the interval bounded?
                  </mlhim2:description>
                </owl:DatatypeProperty>
              </xs:appinfo>
-->            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="upper-bounded" type="xs:boolean">
            <xs:annotation>
              <xs:documentation xml:lang="en-us">
