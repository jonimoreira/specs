            less_than_or_equal : value is less_than_or_equal to the magnitude
            
            greater_than_or_equal : value is greater_than_or_equal to the magnitude
            
            approximate : value is the approximately the magnitude
            
          </mlhim2:description>
          <owl:oneOf rdf:parseType="Collection">
            <owl:Thing rdf:about="&mlhim2;approximate"/>
            <owl:Thing rdf:about="&mlhim2;equal"/>
            <owl:Thing rdf:about="&mlhim2;greater_than"/>
            <owl:Thing rdf:about="&mlhim2;greater_than_or_equal"/>
            <owl:Thing rdf:about="&mlhim2;less_than"/>
            <owl:Thing rdf:about="&mlhim2;less_than_or_equal"/>
          </owl:oneOf>
        </owl:Class>
        <rdf:Description rdf:about="&mlhim2;approximate"/>
        <rdf:Description rdf:about="&mlhim2;equal"/>
        <rdf:Description rdf:about="&mlhim2;greater_than"/>
        <rdf:Description rdf:about="&mlhim2;greater_than_or_equal"/>
        <rdf:Description rdf:about="&mlhim2;less_than"/>
        <rdf:Description rdf:about="&mlhim2;less_than_or_equal"/>
      </xs:appinfo>
-->    </xs:annotation>
    <xs:restriction base="xs:string">
