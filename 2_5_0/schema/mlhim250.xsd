<!--                <xs:appinfo>
                  <owl:DatatypeProperty rdf:about="&mlhim2;text-content">
                    <rdfs:domain rdf:resource="&mlhim2;DvFileType"/>
                    <rdfs:range rdf:resource="&xsd;string"/>
                    <mlhim2:description xml:lang="en-us">
