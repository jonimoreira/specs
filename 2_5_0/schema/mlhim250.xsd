                </owl:ObjectProperty>
              </xs:appinfo>
-->            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="upper" type="mlhim2:InvlType">
            <xs:annotation>
              <xs:documentation xml:lang="en-us"> 
                Defines the upper value of the interval. 
              </xs:documentation>
<!--              <xs:appinfo>
                <owl:ObjectProperty rdf:about="&mlhim2;DvIntervalType/upper">
                  <rdfs:domain rdf:resource="&mlhim2;DvIntervalType"/>
                  <rdfs:range rdf:resource="&mlhim2;InvlType"/>
                  <mlhim2:description xml:lang="en-us">
                    Defines the upper value of the interval. 
                  </mlhim2:description>
